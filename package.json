{
  "name": "finslive3",
  "version": "3.6.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "del": "powershell Remove-Item //gispolarisdev1v/c$/inetpub/wwwroot/finslive/* -Recurse -Force",
    "move": "powershell Copy-Item –Path dist/* –Destination //gispolarisdev1v/c$/inetpub/wwwroot/finslive -Recurse -Force",
    "test:e2e": "vue-cli-service test:e2e",
    "copy": "powershell Copy-Item –Path finslive_bundle.zip –Destination //gisags2v/c$/temp",
    "cpassets": "powershell Copy-Item –Path node_modules/@arcgis/core/assets/* –Destination public/assets -Recurse -Force",
    "move2dev": "powershell Remove-Item //gispolarisdev1v/c$/www/* -Recurse -Force && powershell Copy-Item –Path dist/* –Destination //gispolarisdev1v/c$/www -Recurse -Force",
    "move2prod": "powershell Remove-Item //gisappnet1v/d$/www/finslive/* -Recurse -Force && powershell Copy-Item –Path dist/* –Destination //gisappnet1v/d$/www/finslive -Recurse -Force",
    "pub2dev": "npm run build && npm run move2dev",
    "pub2prod": "npm run build && npm run move2prod",
    "zip": "powershell Remove-Item finslive_bundle.zip -Recurse -Force && powershell Compress-Archive -Path dist/* -DestinationPath finslive_bundle.zip",
    "zipnodelete": "powershell Compress-Archive -Path dist/* -DestinationPath finslive_bundle.zip",
    "build-report": "vue-cli-service build --report"
  },
  "dependencies": {
    "@arcgis/core": "^4.23.7",
    "@mdi/font": "^6.5.95",
    "@turf/interpolate": "^6.5.0",
    "axios": "^0.26.0",
    "core-js": "^3.6.5",
    "d3": "^7.3.0",
    "fontsource-roboto": "^4.0.0",
    "iso8601-duration": "^1.3.0",
    "register-service-worker": "^1.7.1",
    "txml": "^5.1.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuelidate": "^0.7.7",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-analytics": "^5.22.1",
    "vue-cli-plugin-vuetify": "~2.4.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  }
}
